main:
    field.        
        
ascii:
    r1l>i_ r_b>r_
    
string:
    r1l>i_ r_b>s_
    
int:
    r1l>i_ r_b>i_
    
hex:
    r1l>i_ r_b>h_
    
float:
    r1l>i_ r_b>f_
    
timestamp:
    # r1l>i_ r_b>t_
    int. # timestamps are actually more useful as integers, not date-strings
    
field: 
    # tail-recurses to gobble up all available fields. 
    # (last field exits program)
    r1l>i_ field_. field. 
    
trap:
    lEUNKNOWN>r! c>r! lCOLON>r! hex. c>r! lEOS>r!

